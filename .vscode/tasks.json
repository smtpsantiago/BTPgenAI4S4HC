{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm-install",
      "type": "process",
      "command": "npm",
      "args": [
        "install"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "BuildMDK",
      "type": "process",
      "command": "npx",
      "args": [
        "mdk",
        "build",
        "--target",
        "module",
        "--ui5",
        "--recursive"
      ],
      "dependsOn": [
        "npm-install"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "type": "dpt.ptdeploy",
      "label": "Deploy josebecerra_07_a19",
      "taskType": "Deploy"
    },
    {
      "type": "dpt.ptdeploy",
      "label": "Enable Discovery and Deploy josebecerra_07_a19",
      "taskType": "Deploy",
      "enableDiscovery": "true"
    },
    {
      "label": "DeployToHANADB:.env1",
      "type": "shell",
      "command": "eval $(cat ${workspaceFolder}/env/.env1) cds deploy --to hana --auto-undeploy",
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "PreLaunchTask for Run josebecerra_07_a19",
      "type": "process",
      "command": "npx",
      "args": [
        "dev-cap-tools",
        "gen-entrypoint",
        "--ui5version",
        "1.124.0",
        "--ui5theme",
        "sap_belize"
      ],
      "dependsOn": [
        "BuildMDK",
        "DeployToHANADB:.env1"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}